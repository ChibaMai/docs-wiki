{
  "title": "docs · wiki",
  "author": "Chiba Mai",
  "description": "wiki,文档,文章,面试题,代码功能",
  "language": "zh-hans",
  "plugins": [
    "-sharing",
    "-lunr", 
    "-search",
    "-code",
    "-pageview-count",
    "-popup",
    "-splitter",
    "-highlight",
    "-change_girls",
    "-auto-scroll-table",
    "insert-logo",
    "github",
    "github-buttons",
    "sharing-plus",
    "back-to-top-button",
    "copy-code-button",
    "katex",
    "edit-link",
    "emphasize",
    "hide-element",
    "page-toc-button",
    "tbfed-pagefooter",
    "advanced-emoji",
    "expandable-chapters",
    "search-pro",
    "accordion",
    "flexible-alerts",
    "livereload",
    "prism",
    "lightbox"
  ],
  "pluginsConfig": {
    "github": {
      "url": "https://github.com/ChibaMai/docs-wiki"
    },
    "insert-logo": {
      "url": "https://blog.chibamai.xyz/_nuxt/img/Avatar.8f62f10.jpg",
      "style": "background: none; max-height: 100px; min-height: 100px; border-radius: 50%;"
    },
    "github-buttons": {
      "buttons": [{
        "user": "ChibaMai",
        "repo": "docs-wiki",
        "type": "star",
        "count": true,
        "size": "lg"
      }]
    },
    "hide-element": {
      "elements": [
        ".gitbook-link"
      ]
    },
    "edit-link": {
      "base": "https://github.com/ChibaMai/docs-wiki/blob/master",
      "label": "编辑本页"
    },
    "favicon":{
      "shortcut":"favicon.ico",
      "bookmark":"favicon.ico"
    },
    "sharing": {
      "qzone": false,
      "weibo": false,
      "twitter": true,
      "facebook": true,
      "google": false,
      "line": false,
      "whatsapp": false,
      "douban": false,
      "all": [
        "douban",
        "facebook",
        "google",
        "linkedin",
        "twitter",
        "weibo",
        "whatsapp"
      ]
    },
    "page-toc-button": {
      "maxTocDepth": 4,
      "minTocSize": 4
    },
    "tbfed-pagefooter": {
      "copyright": "Chiba Mai © ibooker.org.cn 2020",
      "modify_label": "该文件修订时间: ",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    }
  },
  "styles": {
    "website": "/gitbook/website/website.css"
  }
}
